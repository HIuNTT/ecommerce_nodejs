# swagger
SWAGGER_ENABLE = true
SWAGGER_PATH = api-docs
SWAGGER_VERSION = 1.0

# security
JWT_SECRET = ""
JWT_EXPIRE = 15m
REFRESH_TOKEN_SECRET = ""
REFRESH_TOKEN_EXPIRE = 7d
REFRESH_TOKEN_EXPIRE = 7d
REFRESH_TOKEN = 'token'

# cookie
COOKIE_DOMAIN = ''
COOKIE_PATH = '/'
COOKIE_MAX_AGE = 7d
COOKIE_HTTP_ONLY = true
COOKIE_SECURE = false
COOKIE_SAME_SITE = 'lax'

